     setInputErro('');\n        } catch(err) {\n            setInputErro('Nenhum reposit贸rio encontrado.');\n        }\n    }\n\n\n    return (\n    <>\n        <img src={logoImg} alt=\"GitExplorer logo\"/>\n        <Title>Explore reposit贸rios no Github</Title>\n        <Form hasError={!!inputErro} onSubmit={handleAddRepository}>\n            <input\n                value={newRepo}\n                onChange={(e) => setNewRepo(e.target.value)}\n                placeholder=\"Digite aqui o nome do reposit贸rio\"\n            />\n            <button type=\"submit\">Pesquisar</button>\n        </Form>\n        {inputErro && <Error>{inputErro}</Error>}\n        <Repositories>\n            {\n                repositories.map( repository => (\n                    <a key={repository.full_name} href=\"#\"> \n                            <img src={repository.owner.avatar_url} alt={repository.owner.login} />\n                        <div>\n                            <strong>\n                                {repository.full_name}\n                            </strong>\n                            <p>\n                                {repository.description}\n                            </p>\n                        </div>\n                        <FiChevronRight size={20} />\n                    </a>\n                ))\n            }\n        </Repositories>\n    </>\n    );\n}\n\nexport default Dashboard;","/Users/lucasscherpel/GoStack/nivelDois/react/primeiroProjetoComReact/github-explorer/github-explorer/src/index.tsx",[],"/Users/lucasscherpel/GoStack/nivelDois/react/primeiroProjetoComReact/github-explorer/github-explorer/src/routes/index.tsx",[],"/Users/lucasscherpel/GoStack/nivelDois/react/primeiroProjetoComReact/github-explorer/github-explorer/src/Dashboard/styles.ts",[],"/Users/lucasscherpel/GoStack/nivelDois/react/primeiroProjetoComReact/github-explorer/github-explorer/src/styles/global.ts",[],"/Users/lucasscherpel/GoStack/nivelDois/react/primeiroProjetoComReact/github-